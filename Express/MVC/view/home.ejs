<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home Page</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">  
</head>
<body>
    <!-- <h1 style="text-align: center;">Hello from Server.</h1> -->

    <div class="container">
        <div class="row">
            <% users.forEach(user => { %>
    
                    <div class="col-3 m-4" >
                         <div class="card" style="width: 20rem;" >
                            <div class="card-body">
                                <img src="../uploads/<%= user.profileimage%>" alt="" width="100%" height="400px">
                                <p class="card-text">Email:<%= user.id %></p>
                                <h2 class="card-title">FirstName:<%= user.firstName %></h2>
                                <h2 class="card-subtitle mb-2 text-body-secondary">LastName:<%= user.lastName %></h2>
                                <p class="card-text">Email:<%= user.email %></p>    
                                <p class="card-text">Job_Title:<%= user.job_Title %></p>
                                <p class="card-text">gender:<%= user.gender %></p>
                            </div>
                        </div>
                    </div>
            <% }) %>

        </div>
    </div>

   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>
</html> 